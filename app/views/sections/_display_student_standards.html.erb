<% subheadings.each do |sh| %>
    <h3><%= sh %></h3>
    <table>
        <tr>
            <th>Standard</th>
            <th>How I'm feeling</th>
            <th>Comments</th>
            <th>Evidence</th>
            <th>Edit</th>
        </tr>
    <% sorted_standards.each do |ss| %>
        <% if ss.subheading == sh %>
            <tr>
                <td><%= ss.description %> </td>
                <td><%= ss.progress_level %></td>
                <td><%= ss.comments %></td>
                <td><%= ss.evidence %></td>
                <td><%= link_to "Edit Progress", edit_student_standard_path(ss) %></td>
            </tr>
        <% end %> 
    <%end%>
    </table>
<%end%>